#include <iostream>

using namespace std;

 class employee
 {
     public:
     int id;
     string name;
     float sal;
 
 void insert(int i, string n, float s)
 {
     id=i;
     name=n;
     sal=s;
     
 }
 
 void display()
 {
     
    cout<<id<<" "<<name<<" "<<sal<<endl;
    
 }
 };
 int main(void){
     employee e1;
     employee e2;
     e1.insert(123,"aman",50000);
     e2.insert(214,"singh",51000);
 e1.display();
 e2.display();
 return 0;
     
 }
